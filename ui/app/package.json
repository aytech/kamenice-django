{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "^3.6.9",
    "antd": "^4.21.3",
    "graphql": "^16.5.0",
    "i18next": "^21.8.10",
    "i18next-http-backend": "^1.4.0",
    "interactjs": "^1.10.14",
    "react": "^18.2.0",
    "react-apollo": "^3.1.5",
    "react-calendar-timeline": "^0.28.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.17.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "schema": "npx apollo client:download-schema --endpoint=http://localhost:8000/api schema.json",
    "generate": "npx apollo client:codegen --localSchemaFile=schema.json,local-schema.graphql --includes=src/**/*.ts --globalTypesFile=./src/lib/graphql/globalTypes.ts --target=typescript",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "init": "graphql-codegen init",
    "generate-schema": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-operations": "2.4.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.16",
    "@types/graphql": "^14.5.0",
    "@types/react-calendar-timeline": "^0.26.6",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "apollo": "^2.34.0"
  },
  "proxy": "http://localhost:8000"
}
