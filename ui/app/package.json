{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@apollo/client": "^3.3.20",
    "antd": "^4.16.3",
    "graphql": "^15.5.0",
    "interactjs": "^1.10.11",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-calendar-timeline": "^0.27.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.3"
  },
  "scripts": {
    "codegen:schema": "npx apollo client:download-schema --endpoint=http://localhost:8000/api",
    "codegen:generate": "npx apollo client:codegen --localSchemaFile=schema.json --includes=src/**/*.ts --globalTypesFile=./src/lib/graphql/globalTypes.ts --target=typescript",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-calendar-timeline": "^0.26.4",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.7"
  },
  "proxy": "http://localhost:8000"
}
