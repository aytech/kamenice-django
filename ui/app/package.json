{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "@apollo/client": "^3.5.6",
    "antd": "^4.18.2",
    "graphql": "^16.2.0",
    "i18next": "^21.6.5",
    "i18next-http-backend": "^1.3.1",
    "interactjs": "^1.10.11",
    "react": "^17.0.2",
    "react-apollo": "^3.1.5",
    "react-calendar-timeline": "^0.27.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "schema": "npx apollo client:download-schema --endpoint=http://localhost:8000/api schema.json",
    "generate": "npx apollo client:codegen --localSchemaFile=schema.json,local-schema.graphql --includes=src/**/*.ts --globalTypesFile=./src/lib/graphql/globalTypes.ts --target=typescript",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/react-calendar-timeline": "^0.26.5",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "apollo": "^2.33.9"
  },
  "proxy": "http://localhost:8000"
}
